<Page x:Class="YahtClub.FitPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YahtClub"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="FitPage" Background="White">
    <Grid>
        <Button x:Name="btnBoats" Content="Boats" HorizontalAlignment="Center" Margin="0,0,400,230" VerticalAlignment="Center" Width="353" Height="130"
            FontSize="20" Click="btnBoats_Click"/>
        <Button x:Name="btnAcc" Content="Accessory" HorizontalAlignment="Center" Margin="400,0,0,230" VerticalAlignment="Center" Width="353" Height="130"
            FontSize="20" Click="btnAccessory_Click"/>
        <Button x:Name="btnFit" Content="Fit" HorizontalAlignment="Center" Margin="0,200,400,100" VerticalAlignment="Center" Width="353" Height="130"
            FontSize="20" Click="btnFit_Click"/>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Center" Margin="0,500,500,10" VerticalAlignment="Center" Width="246" Height="36" 
            FontSize="20" Click="btnBack_Click" />

    </Grid>
</Page>
