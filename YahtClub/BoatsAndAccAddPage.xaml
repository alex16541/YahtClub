<Window x:Class="YahtClub.BoatsAndAccAddPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YahtClub"
      mc:Ignorable="d" 
      Height="200 " Width="400"
      Title="BoatsAndAccAddPage" ResizeMode="NoResize">

    <Grid>
        <Label HorizontalContentAlignment="Right" Content="Boat:" HorizontalAlignment="Left" Margin="131,43,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.462,0.848"/>
        <Label HorizontalContentAlignment="Right" Content="Accessory:" HorizontalAlignment="Left" Margin="103,70,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbBoat" HorizontalAlignment="Left" Margin="173,43,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cbAccessory" HorizontalAlignment="Left" Margin="173,70,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="200,135,0,0" VerticalAlignment="Top" Width="184" Height="26" FontSize="16" Click="btnAdd_Click"/>
        <Button x:Name="btnReg_Copy" Content="Cancel" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top" Width="184" Height="26" FontSize="16" Click="btnClose_Click" />
    </Grid>
</Window>
